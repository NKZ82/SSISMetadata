using System;
using System.Data;
using System.Collections.Generic;
using System.DirectoryServices.AccountManagement;
using Microsoft.SqlServer.Dts.Pipeline.Wrapper;
using Microsoft.SqlServer.Dts.Runtime.Wrapper;
#endregion

[Microsoft.SqlServer.Dts.Pipeline.SSISScriptComponentEntryPointAttribute]
public class ScriptMain : UserComponent
{
    public override void Input0_ProcessInputRow(Input0Buffer Row)
    {
        var recipientList = new List<string>();

        if (Row.Recipient_IsNull)
            return;

        var recipients = Row.Recipient.Split(new[] { ";" }, StringSplitOptions.RemoveEmptyEntries);
        foreach (var recipient in recipients)
        {
            if (recipient.Contains("@"))  // Email address
                recipientList.Add(recipient);
            else // Groupe AD
            {
                var groupRecipients = GetEmailAddressesInActiveDirectoryGroup(recipient.ToUpper());
                recipientList.AddRange(groupRecipients);
            }
        }

        Row.RecipientList = String.Join(";", recipientList);
    }

    private List<string> GetEmailAddressesInActiveDirectoryGroup(string activeDirectoryGroup)
    {
        var domain = "CORP";
        var emailAddresses = new List<string>();

        using (PrincipalContext ctx = new PrincipalContext(ContextType.Domain, domain))
        {
            GroupPrincipal grp = GroupPrincipal.FindByIdentity(ctx, IdentityType.Name, activeDirectoryGroup);

            if (grp != null)
            {
                var members = grp.GetMembers(true);

                foreach (var p in grp.GetMembers(true))
                {
                    UserPrincipal user = UserPrincipal.FindByIdentity(ctx, IdentityType.SamAccountName, p.SamAccountName);
                    if (user.EmailAddress != null)
                    {
                        emailAddresses.Add(user.EmailAddress);
                    }
                }
                grp.Dispose();
            }
        }

        return emailAddresses;
    }
}
]]></arrayElement>
