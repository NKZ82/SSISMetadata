    <DTS:Variable
      DTS:CreationName=""
      DTS:DTSID="{7BD3622D-4D60-463D-937E-AFB6A52120D4}"
      DTS:EvaluateAsExpression="True"
      DTS:Expression="&quot;SELECT distinct  LTRIM(RTRIM(CAST(Finance_HFMEntity_Code AS NVARCHAR(12)))) AS [HFMEntity_Code]&#xA;      ,MAX(LTRIM(RTRIM(CAST(Name_EN AS NVARCHAR(80))))) AS Name_EN &#xA;      ,MAX(LTRIM(RTRIM(CAST(Description_EN AS NVARCHAR(80))))) AS Description_EN&#xA;      ,MAX(LTRIM(RTRIM(CAST(Name_FR AS NVARCHAR(80))))) AS Name_FR &#xA;      ,MAX(LTRIM(RTRIM(CAST(Description_FR AS NVARCHAR(80))))) AS Description_FR&#xA;      ,[IC_DM_BUSINESS_PROCESS_INSTANCE_ID]&#xA;      ,[IC_DM_BUSINESS_PROCESS_INSTANCE_ITERATION]&#xA;FROM   [entity].[Finance_HFMEntity] A&#xA;   INNER JOIN &#xA;   (SELECT Finance_HFMEntity_Code as maxId, &#xA;   max(IC_DM_LOADING_DATE) as maxLoadingDate &#xA;   FROM   [entity].[Finance_HFMEntity]&#xA;   GROUP BY Finance_HFMEntity_Code) B&#xA;ON A.Finance_HFMEntity_Code = B.maxId&#xA;AND A.IC_DM_LOADING_DATE = B.maxLoadingDate&#xA;where  [IC_DM_TIMESTAMP_DELETE] is null&#xA;group by Finance_HFMEntity_Code&#xA;,[IC_DM_BUSINESS_PROCESS_INSTANCE_ID]  &#xA;,[IC_DM_BUSINESS_PROCESS_INSTANCE_ITERATION]&quot;"
      DTS:IncludeInDebugDump="2345"
      DTS:Namespace="User"
      DTS:ObjectName="sql_GetDataFromDS">
      <DTS:VariableValue
        DTS:DataType="8">SELECT distinct  LTRIM(RTRIM(CAST(Finance_HFMEntity_Code AS NVARCHAR(12)))) AS [HFMEntity_Code]
      ,MAX(LTRIM(RTRIM(CAST(Name_EN AS NVARCHAR(80))))) AS Name_EN 
      ,MAX(LTRIM(RTRIM(CAST(Description_EN AS NVARCHAR(80))))) AS Description_EN
      ,MAX(LTRIM(RTRIM(CAST(Name_FR AS NVARCHAR(80))))) AS Name_FR 
      ,MAX(LTRIM(RTRIM(CAST(Description_FR AS NVARCHAR(80))))) AS Description_FR
      ,[IC_DM_BUSINESS_PROCESS_INSTANCE_ID]
      ,[IC_DM_BUSINESS_PROCESS_INSTANCE_ITERATION]
FROM   [entity].[Finance_HFMEntity] A
   INNER JOIN 
   (SELECT Finance_HFMEntity_Code as maxId, 
   max(IC_DM_LOADING_DATE) as maxLoadingDate 
   FROM   [entity].[Finance_HFMEntity]
   GROUP BY Finance_HFMEntity_Code) B
ON A.Finance_HFMEntity_Code = B.maxId
AND A.IC_DM_LOADING_DATE = B.maxLoadingDate
where  [IC_DM_TIMESTAMP_DELETE] is null
group by Finance_HFMEntity_Code
,[IC_DM_BUSINESS_PROCESS_INSTANCE_ID]  
,[IC_DM_BUSINESS_PROCESS_INSTANCE_ITERATION]</DTS:VariableValue>
