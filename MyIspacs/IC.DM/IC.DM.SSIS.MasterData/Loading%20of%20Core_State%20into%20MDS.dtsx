    <DTS:Variable
      DTS:CreationName=""
      DTS:DTSID="{014C1616-CD87-4312-87F1-DE1D8CC266A8}"
      DTS:EvaluateAsExpression="True"
      DTS:Expression="&quot;SELECT DISTINCT CAST(s.Core_State_Code AS NVARCHAR(30)) AS Core_State_Code,&#xA;&#x9;CAST(s.Name_EN AS NVARCHAR(100)) AS Name_EN,&#xA;&#x9;CAST(s.Name_FR AS NVARCHAR(100)) AS Name_FR,&#xA;&#x9;CAST(s.Core_Country_Code AS NVARCHAR(30)) AS Core_Country_Code&#xA;FROM cleansing.Procurement_Vendor v&#xA;LEFT JOIN entity.Core_State s&#xA;&#x9;ON v.Core_State_Code = s.Core_State_Code&#xA;WHERE v.IC_DM_BUSINESS_PROCESS_INSTANCE_ID = '&quot;+@[$Package::BusinessProcessInstanceID]+&quot;'&#xA;&#x9;AND v.IC_DM_BUSINESS_PROCESS_INSTANCE_ITERATION = &quot;+(DT_WSTR, 255) @[$Package::BusinessProcessInstanceIteration]+&quot;&#xA;&#x9;AND v.Core_State_Code_CleansingStatus = 'NOT FOUND'&#xA;&#x9;AND v.AddressValidationStatus = 'VALID'&#xA;&#x9;AND v.Core_State_Code IS NOT NULL&#xA;ORDER BY&#xA;&#x9;CAST(s.Core_State_Code AS NVARCHAR(30))&quot;"
      DTS:IncludeInDebugDump="2345"
      DTS:Namespace="User"
      DTS:ObjectName="sql_DSGetCleansingData">
      <DTS:VariableValue
        DTS:DataType="8">SELECT DISTINCT CAST(s.Core_State_Code AS NVARCHAR(30)) AS Core_State_Code,
	CAST(s.Name_EN AS NVARCHAR(100)) AS Name_EN,
	CAST(s.Name_FR AS NVARCHAR(100)) AS Name_FR,
	CAST(s.Core_Country_Code AS NVARCHAR(30)) AS Core_Country_Code
FROM cleansing.Procurement_Vendor v
LEFT JOIN entity.Core_State s
	ON v.Core_State_Code = s.Core_State_Code
WHERE v.IC_DM_BUSINESS_PROCESS_INSTANCE_ID = '20171116-EL-01'
	AND v.IC_DM_BUSINESS_PROCESS_INSTANCE_ITERATION = 1
	AND v.Core_State_Code_CleansingStatus = 'NOT FOUND'
	AND v.AddressValidationStatus = 'VALID'
	AND v.Core_State_Code IS NOT NULL
ORDER BY
	CAST(s.Core_State_Code AS NVARCHAR(30))</DTS:VariableValue>
    </DTS:Variable>
