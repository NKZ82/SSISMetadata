    <DTS:Variable
      DTS:CreationName=""
      DTS:DTSID="{7E3156E8-308E-40F6-BCB8-9604AD02293D}"
      DTS:EvaluateAsExpression="True"
      DTS:Expression="&quot;-- Variables&#xA;DECLARE @Version_ID INT&#xA;DECLARE @User_ID INT&#xA;DECLARE @Entity_ID INT&#xA;&#xA;-- Get the latest version of the model&#xA;SELECT @Version_ID = MAX(ID)&#xA;FROM mdm.viw_SYSTEM_SCHEMA_VERSION&#xA;WHERE Model_ID IN (&#xA;&#x9;SELECT [ID]&#xA;&#x9;FROM [mdm].[viw_SYSTEM_SCHEMA_MODEL]&#xA;&#x9;WHERE [Name] = 'DM_MDM'&#xA;)&#xA;&#xA;-- Get UserID &#xA;SELECT @User_ID = 1 -- super user&#xA;&#xA;-- Get EntityID&#xA;SELECT @Entity_ID = ID &#xA;FROM mdm.tblEntity &#xA;WHERE [Name] = 'Core - State'&#xA;&#xA;-- Validate entity&#xA;EXECUTE mdm.udpValidateEntity @User_ID,&#xA;&#x9;@Version_ID,&#xA;&#x9;@Entity_ID&quot;"
      DTS:IncludeInDebugDump="2345"
      DTS:Namespace="User"
      DTS:ObjectName="sql_MDSValidateEntity">
      <DTS:VariableValue
        DTS:DataType="8">-- Variables
DECLARE @Version_ID INT
DECLARE @User_ID INT
DECLARE @Entity_ID INT

-- Get the latest version of the model
SELECT @Version_ID = MAX(ID)
FROM mdm.viw_SYSTEM_SCHEMA_VERSION
WHERE Model_ID IN (
	SELECT [ID]
	FROM [mdm].[viw_SYSTEM_SCHEMA_MODEL]
	WHERE [Name] = 'DM_MDM'
)

-- Get UserID 
SELECT @User_ID = 1 -- super user

-- Get EntityID
SELECT @Entity_ID = ID 
FROM mdm.tblEntity 
WHERE [Name] = 'Core - State'

-- Validate entity
EXECUTE mdm.udpValidateEntity @User_ID,
	@Version_ID,
	@Entity_ID</DTS:VariableValue>
